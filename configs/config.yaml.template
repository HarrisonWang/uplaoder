server:
  port: "${SERVER_PORT}"
  read_timeout: 10s
  write_timeout: 30s

upload:
  path: "${UPLOAD_PATH}"
  media_url_prefix: "${MEDIA_URL_PREFIX}"
  max_size: 10485760

ocr:
  endpoint: "${OCR_ENDPOINT}"
  alibaba_cloud_access_key_id: "${ALIBABA_CLOUD_ACCESS_KEY_ID}"
  alibaba_cloud_access_key_secret: "${ALIBABA_CLOUD_ACCESS_KEY_SECRET}" 